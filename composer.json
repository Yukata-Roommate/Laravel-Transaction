{
    "name": "yukata-roommate/laravel-transaction",
    "description": "Easy to use transaction management for Laravel",
    "type": "library",
    "license": "MIT",
    "version": "1.0.4",
    "autoload": {
        "psr-4": {
            "YukataRm\\Laravel\\Transaction\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "YukataRm\\Laravel\\Transaction\\Provider\\FacadeServiceProvider"
            ],
            "aliases": {
                "Transaction": "YukataRm\\Laravel\\Transaction\\Facade\\Transaction"
            }
        }
    },
    "authors": [
        {
            "name": "Yukata Roommate",
            "email": "contact@yukata-roommate.com"
        }
    ],
    "require": {
        "php": ">=8.2.0",
        "laravel/framework": "^11.0",
        "yukata-roommate/laravel-provider": "^1.0.0"
    },
    "minimum-stability": "dev"
}